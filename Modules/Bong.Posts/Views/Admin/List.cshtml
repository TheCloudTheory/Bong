@{
    Layout = "_Layout";
}


<div class="columns module-header">
    <div class="column">
        <h1>Posts</h1>
    </div>
    <div class="column text-right">
        <a href="#" class="btn btn-primary">Create a post</a>
    </div>
</div>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>name</th>
            <th>genre</th>
            <th>release date</th>
        </tr>
    </thead>
    <tbody>
        <tr class="active">
            <td>The Shawshank Redemption</td>
            <td>Crime, Drama</td>
            <td>14 October 1994</td>
        </tr>
    </tbody>
</table>